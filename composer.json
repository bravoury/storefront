{
    "name": "xetam/storefront",
    "description": "Storefront package for xetam.",
    "keywords": ["lavalite", "Storefront"],
    "license": "MIT",
    "authors": [
        {
            "name": "Lavalite",
            "email": "info@lavalite.org"
        }
    ],
    "require": {
        "php": ">=5.5.9"
    },
    "autoload": {
        "psr-4": {
            "Xetam\\Storefront\\": "src/Xetam/Storefront"
        }
    },
       "extra": {
        "laravel": {
            "providers": [
                "Xetam\\Storefront\\Providers\\StorefrontServiceProvider"
            ],
            "aliases": {
                "StorefrontDavy"       : "Xetam\\Storefront\\Facades\\Storefront"
            }
        }
    },
    "minimum-stability": "dev"
}
